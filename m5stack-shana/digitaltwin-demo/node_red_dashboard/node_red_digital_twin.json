arseFloat(msg.payload.Humidity) || 'N/A' };\n    let msg4 = {\n        topic: \"sensor_1_Altitude\",\n        payload: parseFloat(msg.payload.Altitude) || 'N/A' };\n    let msg5 = {\n        topic: \"sensor_1_Pressure\", \n        payload: parseFloat(msg.payload.Pressure) || 'N/A' };\n\n    return [msg1, msg2, msg3, msg4, msg5];\n} else {\n    // Handle case where msg.payload is not in the expected format\n    //let msg1 = { payload: msg.payload }\n    //return [msg1, msg1, msg1, msg1, msg1];\n}",
